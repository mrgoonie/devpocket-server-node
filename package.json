{
  "name": "devpocket-server-node",
  "version": "1.0.0",
  "description": "DevPocket - Mobile-first cloud IDE backend server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "dotenv -e .env -- jest --watch",
    "test:coverage": "dotenv -e .env -- jest --coverage",
    "test:unit": "jest --config jest.unit.config.js",
    "test:unit:watch": "jest --config jest.unit.config.js --watch",
    "test:unit:coverage": "jest --config jest.unit.config.js --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "check-types": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/prisma/seed.ts",
    "db:push:prod": "dotenv -e .env.prod -- prisma db push",
    "db:migrate:prod": "dotenv -e .env.prod -- prisma migrate deploy",
    "db:studio:prod": "dotenv -e .env.prod -- prisma studio",
    "db:seed:prod": "dotenv -e .env.prod -- tsx src/prisma/seed.ts",
    "templates:load": "tsx scripts/load_templates.ts",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run",
    "docker:build": "docker build -t devpocket-server .",
    "docker:run": "docker run -p 8000:8000 devpocket-server"
  },
  "keywords": [
    "cloud-ide",
    "mobile-first",
    "kubernetes",
    "websocket",
    "terminal",
    "docker",
    "typescript",
    "express"
  ],
  "author": "DevPocket Team",
  "license": "MIT",
  "dependencies": {
    "@kubernetes/client-node": "^0.20.0",
    "@prisma/client": "^5.7.1",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/ws": "^8.5.10",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^9.4.1",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "resend": "^2.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "yaml": "^2.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.9.4",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^5.7.1",
    "semantic-release": "^24.2.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.6.0"
}
